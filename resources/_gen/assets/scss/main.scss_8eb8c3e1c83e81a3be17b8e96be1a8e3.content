:root{--cream:#b16707a7;--light-orange:#F4E4BC;--medium-orange:#E6B800;--rich-orange:#D97706;--deep-orange:#B45309;--light-brown:#D2B48C;--medium-brown:#A0522D;--rich-brown:#8B4513;--deep-brown:#654321;--accent-orange:#F59E0B;--accent-brown:#92400E;--text-primary:#1A0F08;--text-secondary:#3D2F1F;--text-light:#5D4E37;--shadow-subtle:0 2px 8px rgba(139, 69, 19, 0.15);--shadow-medium:0 4px 12px rgba(139, 69, 19, 0.2);--shadow-rich:0 6px 16px rgba(139, 69, 19, 0.25)}body{background-color:var(--cream);background-image:radial-gradient(circle at 20% 80%,rgba(139,69,19,3%) 0%,transparent 50%),radial-gradient(circle at 80% 20%,rgba(139,69,19,3%) 0%,transparent 50%),radial-gradient(circle at 40% 40%,rgba(139,69,19,2%) 0%,transparent 50%),linear-gradient(45deg,transparent 40%,rgba(139,69,19,1%) 50%,transparent 60%);background-size:100% 100%,100% 100%,100% 100%,20px 20px;background-attachment:fixed;color:var(--text-primary);font-family:georgia,times new roman,serif;line-height:1.6;margin:0;padding:0;max-width:800px;margin:0 auto;padding:2rem;position:relative}main{background-color:var(--light-orange);padding:2rem;border-radius:12px;box-shadow:var(--shadow-medium);border:1px solid rgba(139,69,19,.1);position:relative;z-index:1}h1,h2,h3,h4,h5,h6{color:var(--deep-brown);margin-top:2rem;margin-bottom:1rem;font-weight:600;text-shadow:0 1px 2px rgba(139,69,19,.1)}h1{font-size:2.5rem;text-align:center;margin-bottom:2rem;padding-bottom:1rem;border-bottom:3px solid var(--rich-orange)}h2{font-size:2rem;border-bottom:2px solid var(--rich-orange);padding-bottom:.5rem}h3{font-size:1.5rem;color:var(--rich-brown)}a{color:var(--rich-orange);text-decoration:none;transition:all .3s ease;font-weight:500}a:hover{color:var(--deep-orange);text-decoration:underline;text-shadow:0 1px 2px rgba(139,69,19,.2)}a:focus{outline:2px solid var(--rich-orange);outline-offset:2px}a:visited{color:var(--medium-brown)}pre{background-color:var(--deep-brown);color:var(--cream);padding:1rem;border-radius:6px;overflow-x:auto;box-shadow:var(--shadow-rich);border-left:4px solid var(--rich-orange);position:relative}pre::before{content:'';position:absolute;top:0;left:0;right:0;height:3px;background:linear-gradient(90deg,var(--rich-orange),var(--deep-orange));border-radius:6px 6px 0 0}code{background-color:var(--light-orange);color:var(--text-primary);padding:.2rem .4rem;border-radius:4px;font-family:monaco,consolas,monospace;font-weight:500}pre code{background-color:transparent;color:#fff;padding:0}.swiftnote-container{max-width:1e3px;margin:0 auto;padding:2rem}.swiftnote-content{margin-bottom:3rem;padding:2rem;background-color:var(--cream);border-radius:12px;box-shadow:var(--shadow-medium);border:1px solid rgba(139,69,19,.1)}.swiftnote-section{margin-bottom:3rem;padding:2rem;background-color:var(--light-orange);border-radius:12px;box-shadow:var(--shadow-subtle);border:1px solid rgba(139,69,19,.1)}.swiftnote-section h2{color:var(--deep-brown);margin-bottom:1.5rem;padding-bottom:.5rem;border-bottom:2px solid var(--rich-orange);font-size:1.5rem}.keywords-grid{display:grid;grid-template-columns:repeat(auto-fit,minmax(300px,1fr));gap:1rem}.keyword-card{background-color:var(--cream);padding:1.5rem;border-radius:8px;box-shadow:var(--shadow-subtle);border-left:4px solid var(--rich-orange);transition:transform .2s ease,box-shadow .2s ease}.keyword-card:hover{transform:translateY(-2px);box-shadow:var(--shadow-medium)}.keyword-term{color:var(--deep-brown);margin-bottom:.5rem;font-size:1.1rem;font-weight:600}.keyword-definition{color:var(--text-secondary);line-height:1.5;margin:0}.questions-list{display:flex;flex-direction:column;gap:1.5rem}.question-card{background-color:var(--cream);padding:1.5rem;border-radius:8px;box-shadow:var(--shadow-subtle);border-left:4px solid var(--rich-orange)}.question-text{color:var(--deep-brown);margin-bottom:1rem;font-size:1.1rem;font-weight:600}.answer-text{color:var(--text-secondary);line-height:1.6;padding-left:1rem;border-left:2px solid var(--light-orange)}.exercises-list{display:flex;flex-direction:column;gap:1.5rem}.exercise-card{background-color:var(--cream);padding:1.5rem;border-radius:8px;box-shadow:var(--shadow-subtle);border-left:4px solid var(--rich-orange);transition:transform .2s ease,box-shadow .2s ease}.exercise-card:hover{transform:translateY(-2px);box-shadow:var(--shadow-medium)}.exercise-header{display:flex;align-items:center;gap:1rem;margin-bottom:1rem}.exercise-number{background-color:var(--rich-orange);color:#fff;padding:.3rem .8rem;border-radius:20px;font-size:.9rem;font-weight:600}.exercise-title{color:var(--deep-brown);margin:0;font-size:1.1rem;font-weight:600}.exercise-description{color:var(--text-secondary);line-height:1.6;margin-bottom:1rem}.exercise-hint{background-color:rgba(139,69,19,.1);padding:1rem;border-radius:6px;border-left:3px solid var(--rich-orange);color:var(--text-secondary);font-style:italic}@media(max-width:768px){.swiftnote-container{padding:1rem}.swiftnote-content,.swiftnote-section{padding:1.5rem}.keywords-grid{grid-template-columns:1fr}.exercise-header{flex-direction:column;align-items:flex-start;gap:.5rem}}blockquote{background-color:var(--light-orange);border-left:4px solid var(--accent-orange);margin:1.5rem 0;padding:1rem 1.5rem;border-radius:0 8px 8px 0;box-shadow:var(--shadow-subtle);font-style:italic;color:var(--text-secondary)}table{width:100%;border-collapse:collapse;margin:1.5rem 0;background-color:var(--light-orange);border-radius:8px;overflow:hidden;box-shadow:var(--shadow-subtle)}thead{background-color:var(--rich-orange);color:#fff}th,td{padding:.75rem 1rem;text-align:left;border-bottom:1px solid rgba(139,69,19,.1)}th{font-weight:600;text-transform:uppercase;font-size:.9rem;letter-spacing:.5px}tbody tr:hover{background-color:rgba(139,69,19,5%)}ul,ol{margin:1rem 0;padding-left:2rem}li{margin-bottom:.5rem;color:var(--text-secondary)}hr{border:none;height:2px;background:linear-gradient(90deg,transparent,var(--rich-orange),transparent);margin:2rem 0;border-radius:1px}nav{margin-bottom:2rem}nav .menu{list-style:none;padding:0;margin:0;display:flex;justify-content:center;gap:2rem;flex-wrap:wrap}nav .menu li{margin:0}nav .menu li a{padding:.5rem 1rem;background-color:var(--light-orange);border-radius:6px;transition:all .3s ease;display:block}nav .menu li a:hover{background-color:var(--rich-orange);color:#fff;transform:translateY(-2px);box-shadow:var(--shadow-medium)}footer{margin-top:3rem;padding-top:2rem;border-top:2px solid var(--rich-orange);text-align:center;color:var(--text-light);font-size:.9rem}.list-page ul{margin:1.5rem 0;padding-left:0;list-style:none}.list-page li{margin-bottom:1rem;padding:1rem 1.5rem;background-color:var(--light-orange);border-radius:8px;box-shadow:var(--shadow-subtle);border:1px solid rgba(139,69,19,.1);display:flex;align-items:flex-start;gap:1rem}.list-page li:hover{background-color:var(--cream);box-shadow:var(--shadow-medium)}.list-page .date{color:var(--text-light);font-size:.9rem;font-weight:500;min-width:80px}.list-page .post-info{flex:1;display:flex;flex-direction:column;gap:.5rem}.list-page li a{color:var(--rich-orange);text-decoration:none;font-weight:500;font-size:1.1rem}.list-page li a:hover{color:var(--deep-orange);text-decoration:underline}.list-page .summary{color:var(--text-secondary);font-size:.9rem;line-height:1.4;font-style:italic;margin-top:.25rem}@media(max-width:768px){body{padding:1rem}main{padding:1.5rem}h1{font-size:2rem}h2{font-size:1.5rem}nav .menu{flex-direction:column;align-items:center;gap:1rem}.list-page li{flex-direction:column;gap:.5rem}.list-page .date{min-width:auto}}